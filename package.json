{
  "name": "adb_logger",
  "version": "1.0.0",
  "description": "安卓日志查看",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && electron ./dist/main.js",
    "package_begin": "rd /s /q \"./assets/adb/portable\"",
    "package": "electron-packager . --platform=win32 --icon=./icon.ico --overwrite --ignore=\"/(src|README\\.md|LICENSE\\.md|tsconfig\\.json|log\\.txt|icon\\.ico|\\.gitignore|\\.eslintrc)\""
  },
  "main": "dist/main.js",
  "repository": "https://github.com/zt5/adb_logger",
  "keywords": [
    "adb",
    "logger"
  ],
  "author": "zt5",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/node": "^16.7.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "electron-packager": "^15.3.0",
    "electron": "^14.0.0",
    "eslint": "^7.32.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "adm-zip": "^0.5.5",
    "iconv-lite": "^0.6.3",
    "tree-kill": "^1.2.2"
  }
}
